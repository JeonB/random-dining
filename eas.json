{
  "cli": {
    "version": ">= 9.1.0"
  },
  "build": {
    "development": {
      "channel": "development",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "env": {
        "APP_ENV": "preview"
      },
      "channel": "preview",
      "ios": {
        "cocoapods": "1.15.2"
      }
    },
    "production": {
      "env": {
        "APP_ENV": "production"
      },
      "channel": "production",
      "ios": {
        "cocoapods": "1.15.2"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "production"
      }
    },
    "preview": {
      "android": {
        "track": "internal"
      }
    }
  }
}
