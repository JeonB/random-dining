{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": ["*"],
      "@_tests/*": ["src/tests/*"],
      "@_components/*": ["src/components/*"],
      "@_types/*": ["src/types/*"],
      "@_services/*": ["src/services/*"],
      "@env": ["src/types/env.d.ts"]
    },
    "allowSyntheticDefaultImports": true
  }
}
